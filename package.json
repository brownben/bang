{
  "name": "bang",
  "author": "brownben",
  "version": "0.2.0",
  "description": "My Attempt at Creating an Interpreted Language in TypeScript",
  "keywords": [
    "language",
    "typescript"
  ],
  "license": "MIT",
  "repository": "github:brownben/bang",
  "bin": {
    "bang": "./cli.js"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "format": "prettier --write ./**/*",
    "lint": "tsdx lint src test",
    "test": "tsdx test",
    "type-check": "tsc --noEmit",
    "build": "tsdx build -- --transpileOnly"
  },
  "dependencies": {
    "cac": "^6.7.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "prettier": "^2.3.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "**/typescript": "^4.2.4",
    "**/@typescript-eslint/eslint-plugin": "^4.25.0",
    "**/@typescript-eslint/parser": "^4.25.0",
    "**/prettier": "^2.3.0"
  }
}
